    <script>
    /*
     * This is pretty nasty, but works for now.
     */
    function linkify() {
	    foo = document.getElementsByTagName("h2");
	    for (i in foo) {
		    if (foo[i].parentElement != null && foo[i].parentElement.id != null) {
	    		foo[i].innerHTML = "<a href=\"#" + foo[i].parentElement.id + "\">" + foo[i].innerHTML + "</a>"
		    }
	    }
	    foo = document.getElementsByTagName("h1");
	    for (i in foo) {
	    	foo[i].innerHTML = "<a href=\"#\">" + foo[i].innerHTML + "</a>"
	    }
    }
    document.addEventListener('DOMContentLoaded',linkify);
    </script>
